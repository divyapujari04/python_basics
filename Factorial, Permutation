n = int(input("n: "))
r = int(input("r: "))
m = int(input("m: "))

def factorial(x):
    f = 1
    for i in range(1, x + 1):
        f *= i 
    return f 
    
nfact = factorial(n)
rfact = factorial(r)
nrfact = factorial(n-r)


nCr = nfact // (rfact * nrfact)

permutation = nCr % m

print(permutation)
